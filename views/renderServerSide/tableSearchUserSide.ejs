<div class="body table-responsive">
    <table class="table table-hover m-b-0 c_list table-contact-search">
        <tbody>
            <% users.forEach(function(user){  %>
            <tr>
                <td>
                    <img src="assets/images/<%= user.avatar %>" style="object-fit: cover;" width="40" height="40" class="rounded-circle avatar" alt="">
                    <p class="c_name">
                        <%= (user.username.length > 12) ? user.username.substr(0,11)+"..." : user.username %>
                    </p>
                </td>
                <td class="table-phone-mobile">
                    <span class="phone"><i class="zmdi zmdi-phone m-r-10"></i><%= user.phone %></span>
                </td>
                <td class="table-address-mobile">
                    <address><i class="zmdi zmdi-pin"></i><%= user.address %></address>
                </td>
                <td>
                    <button data-id="<%= user._id %>" class="btn btn-default btn-icon btn-simple btn-icon-mini btn-round add-friend">
                        <i class="zmdi zmdi-plus"></i>
                    </button>
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>